#ifndef F_CPU
#define F_CPU 160000000UL  // 16MHz clock speed
#endif

#include <avr/io.h>
#include <util/delay.h>

int main(void)
{
	DDRC = 0xFF; //Make PORTC as OUTPUT
    while (1)	// infinite LOOP
    {
		PORTC = 0xFF; //Turn ON ALL LEDs
		_delay_ms(1000); //1 second delay
		PORTC = 0x00; //Turn OFF ALL LEDs
		_delay_ms(1000); //1 second delay
	}
}

